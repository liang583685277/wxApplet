/* pages/saoyisao/saoyisao.wxss */
.saobody{
	width: 100%;
}
.headnav{
	position: relative;
	width: 100%;
	height: 100rpx;
	line-height: 100rpx;
	text-align: center;
	font-size: 30rpx;
	color: white;
	background-color: #16C39A;
}
.righttap{
	position: absolute;
	top: 0;
	right: 0;
	width: auto;
	height: 100rpx;
	line-height: 100rpx;
	padding: 0 30rpx;
	box-sizing: border-box;
	font-size: 26rpx;
}
.lefttap{
  position: absolute;
	top: 0;
	left: 0;
	width: auto;
	height: 100rpx;
	line-height: 100rpx;
	padding: 0 30rpx;
	box-sizing: border-box;
	font-size: 26rpx;
}